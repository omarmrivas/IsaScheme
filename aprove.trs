(VAR y1 xs2 y22 y2) 
(RULES 
fa(c,ys) -> Pair2(True,cons(y22,ys)) 
f(b2,zs) -> Pair2(b2,cons(y2,zs)) 
bubble(nil) -> Pair2(False,nil) 
bubble(cons(y1,nil)) -> Pair2(False,cons(y1,nil)) 
bubble(cons(y2,cons(y22,xs2))) -> If(less_eq(y2,y22),case_Paira(f,bubble(cons(y22,xs2))),case_Paira(fa,bubble(cons(y2,xs2))))) 
